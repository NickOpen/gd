<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.gateway.gd.domain">

	<class name="InventoryWarn" table="inventorywarn">
		<id name="id">
			<generator class="native" />
		</id>
		<property name="category" />
		<property name="materialsName" />
		<property name="unitName" />
		<property name="lowestInventory" type="double"/>
		<property name="highestInventory" type="double" />
		<property name="safeInventory"  type="double"/>
		<property name="realInventory" type="double" />
		<property name="changeNumber" type="double" />
		<property name="status" />
		<property name="description" />



	</class>

</hibernate-mapping>
